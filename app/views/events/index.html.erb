<h1> This is Events Index page </h1>

<table class="table">
	<tr>
		<th>Name</th>
		<th>location</th>
		<th>date</th>
	</tr>

	<% @events.each do |e| %>
	<tr>
		<td><%= e.title %></td>
		<td><%= e.location.name || "" %></td>
		<td><%= e.event_date %></td>
	</tr>
	<% end %>

